{
"chrome": {
  "default": "121.0",
  "versions": {
    "121.0": {
      "image": "selenoid/vnc:chrome_121.0",
      "port": "4444",
      "path": "/",
      "tmpfs": { "/tmp": "exec" },
      "args": [ "-timeout", "120s" ],
      "cleanup": false,
      "shmSize": 268435456
    }
  }
}
,

  "firefox": {
    "default": "121.0",
    "versions": {
      "121.0": {
        "image": "selenoid/firefox:121.0",
        "port": "4444",
        "path": "/",
        "tmpfs": { "/tmp": "size=512m" },
        "env": ["LANG=en_US.UTF-8", "TZ=Europe/Moscow"],
        "hosts": ["host.docker.internal:host-gateway"]
      }
    }
  }
}
